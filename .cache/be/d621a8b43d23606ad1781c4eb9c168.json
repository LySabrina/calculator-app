inal":{"line":215,"column":4},"generated":{"line":272,"column":4}},{"source":"styles/style.scss","name":null,"original":{"line":216,"column":4},"generated":{"line":273,"column":4}}],"sources":{"styles/style.scss":"@import \"./variables\";\n@import \"./function\";\n@import \"./mixin\";\n@import \"./theme\";\n\n* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n}\n\nbody {\n  min-height: 100vh;\n  font-family: $ff-league-spartan;\n  display: flex;\n  flex-direction: column;\n}\n\nheader {\n  padding: 1.5em;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n\n  #theme-selector {\n    display: flex;\n    align-items: center;\n\n    width: 50%;\n\n    h3 {\n      text-transform: uppercase;\n      letter-spacing: 0.1rem;\n      font-size: 1rem;\n    }\n\n    .theme-switcher {\n      position: relative;\n\n      width: 100%;\n      margin-left: 1em;\n      &__labels {\n        display: flex;\n        justify-content: space-around;\n        label {\n          &:hover {\n            cursor: pointer;\n          }\n        }\n      }\n      &__btns {\n        display: flex;\n        justify-content: space-between;\n        position: relative;\n        background-color: white;\n        width: 100%;\n        height: 2em;\n        border-radius: 1em;\n        padding: 0.5rem;\n        input {\n          opacity: 0;\n          z-index: 1;\n          &:hover {\n            cursor: pointer;\n          }\n        }\n\n        #dark:checked ~ #active-btn {\n          left: 10%;\n        }\n\n        #light:checked ~ #active-btn {\n          left: 40%;\n        }\n        #purple:checked ~ #active-btn {\n          left: 70%;\n        }\n        #active-btn {\n          position: absolute;\n          top: 5px;\n          width: 1.5em;\n          height: 1.5em;\n\n          border-radius: 50%;\n          opacity: 1;\n          transition: left 0.5s ease-in;\n        }\n      }\n    }\n  }\n}\n\nmain {\n  padding: 1em;\n  flex-grow: 1;\n  display: flex;\n  flex-direction: column;\n}\n\n.calculator {\n  flex-grow: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 2rem;\n  &__display {\n    border-radius: 1rem;\n    padding: 1em;\n    font-size: 2rem;\n    font-weight: $fw-bold;\n  }\n\n  &__inputs {\n    flex-grow: 1;\n    border-radius: 1rem;\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n\n    column-gap: 1em;\n    row-gap: 1em;\n    padding: 1.5em;\n\n    &--btn {\n      border: 0;\n      border-radius: 0.5rem;\n      font-weight: $fw-bold;\n      font-size: 2rem;\n\n      &:hover {\n        cursor: pointer;\n        filter: brightness(1.5);\n      }\n\n      &:nth-last-child(2) {\n        grid-column: 1/3;\n      }\n      &:nth-last-child(1) {\n        grid-column: 3/5;\n      }\n    }\n    #reset,\n    #delete {\n      font-size: 1rem;\n    }\n  }\n}\n\n//THEME INTEGRATIONO\n@include themify($themes) {\n  background-color: themed(\"mainBG\");\n  header {\n    color: themed(\"mainText\");\n    .theme-switcher {\n      &__btns {\n        input:checked ~ #active-btn {\n          background-color: themed(\"specialKey1\");\n        }\n      }\n    }\n  }\n\n  .calculator {\n    &__display {\n      background-color: themed(\"screenBG\");\n      color: themed(\"mainText\");\n    }\n    .calculator__inputs {\n      background-color: themed(\"keypadBG\");\n      &--btn {\n        color: themed(\"textDark\");\n        background-color: themed(\"keyBG\");\n        border-bottom: 4px solid themed(\"keyShadow\");\n\n        &:active {\n          transform: translateY(5px);\n          border-bottom: 2px solid themed(\"keyShadow\");\n        }\n      }\n      #equal {\n        background-color: themed(\"specialKey1\");\n        color: themed(\"textLight\");\n        border-bottom: 4px solid themed(\"specialKey1Shadow\");\n        &:active{\n          border-bottom: 2px solid themed(\"specialKey1Shadow\");\n        }\n        \n      }\n      #reset,\n      #delete {\n        background-color: themed(\"specialKey2\");\n        color: themed(\"textLight\");\n        border-bottom: 4px solid themed(\"specialKey2Shadow\");\n\n        &:active{\n          border-bottom: 2px solid themed(\"specialKey2Shadow\");\n        }\n      }\n    }\n  }\n}\n@media screen and (min-width: 30em) {\n  body {\n    justify-content: center;\n    align-items: center;\n  }\n  header {\n    min-width: 40em;\n\n    #theme-selector {\n      max-width: 25%;\n    }\n  }\n\n  main {\n    flex-grow: 0;\n    min-width: 40em;\n    min-height: 40em;\n  }\n}\n","styles/_variables.scss":"$ff-league-spartan: \"League Spartan\", sans-serif;\n$fw-bold: 700;\n","styles/_mixin.scss":"@mixin themify($themes) {\n  @each $name, $value in $themes {\n    .#{$name}-theme {\n      $theme-map: $value !global;\n      @content;\n    }\n  }\n}\n\n\n"},"lineCount":null}},"error":null,"hash":"8a6c65ba0009c0e72aae401116a3220d","cacheData":{"env":{}}}